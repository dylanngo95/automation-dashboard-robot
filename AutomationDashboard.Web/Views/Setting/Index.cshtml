@model IEnumerable<AutomationDashboard.Model.Models.Setting>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutHomeSetting.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Domain)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UrlBuildStart)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UrlBuildEnd)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UrlReportStart)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UrlReportEnd)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Domain)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UrlBuildStart)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UrlBuildEnd)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UrlReportStart)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UrlReportEnd)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
