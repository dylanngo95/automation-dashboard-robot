@model IEnumerable<AutomationDashboard.Model.Models.Build>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutHomeSetting.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.BuildDetails.Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SubProject.FullName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BuildId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Number)
